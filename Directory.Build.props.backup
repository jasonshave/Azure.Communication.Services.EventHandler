<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <AssemblyName>JasonShave.Azure.Communication.Service.$(MSBuildProjectName)</AssemblyName>
        <RootNamespace>JasonShave.Azure.Communication.Service.$(MsBuildProjectName)</RootNamespace>
        <RunCodeAnalysis>false</RunCodeAnalysis>
        <CodeAnalysisRuleSet>$(MSBuildThisFileDirectory)rulesets\compound.ruleset</CodeAnalysisRuleSet>
        <EnableNETAnalyzers>true</EnableNETAnalyzers>
        <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
		<DebugType>Full</DebugType>
		<Nullable>enable</Nullable>
        <Authors>jasonshave@hotmail.com</Authors>
        <Company>Jason Shave</Company>
        <Copyright>Copyright Â© Jason Shave 2022</Copyright>
        <Platform>x64</Platform>
        <Platforms>$(Platform)</Platforms>
        <PlatformTarget>$(Platform)</PlatformTarget>        
    </PropertyGroup>
	<ItemGroup>
		<AdditionalFiles Include="$(MSBuildThisFileDirectory)rulesets\stylecop.json" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Features" Version="4.0.1" />
        <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
          </PackageReference>
	</ItemGroup>
</Project>